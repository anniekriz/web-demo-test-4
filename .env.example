# Copy to .env for local development
# cp .env.example .env

# Local development (docker-compose postgres maps to localhost:5433)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/neo_world_weby

# REQUIRED: set your own strong secret in production
PAYLOAD_SECRET=replace-with-a-long-random-secret

# Optional seed overrides
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=ChangeMe123!
SEED_OWNER_EMAIL=owner@example.com
SEED_OWNER_PASSWORD=ChangeMe123!
